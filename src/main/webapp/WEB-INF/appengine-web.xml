<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>gruuf-webapp</application>
  <version>2</version>
  <sessions-enabled>true</sessions-enabled>
  <threadsafe>true</threadsafe>
  <runtime>java8</runtime>
  <system-properties>
    <property name="log4j2.disable.jmx" value="true"/>
    <property name="OAUTH_GOOGLE_API_KEY" value="${env.OAUTH_GOOGLE_API_KEY}"/>
    <property name="OAUTH_GOOGLE_API_SECRET" value="${env.OAUTH_GOOGLE_API_SECRET}"/>
  </system-properties>
</appengine-web-app>
